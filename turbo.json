{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "!.next/cache/**",
                "dist/**"
            ],
            "env": [
                "NEXT_PUBLIC_*",
                "NODE_ENV"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "format": {
            "outputs": [],
            "cache": false
        },
        "types:check": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "deploy": {
            "dependsOn": [
                "build"
            ],
            "outputs": [],
            "cache": false
        }
    }
}